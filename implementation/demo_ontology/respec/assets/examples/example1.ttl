@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix cccev: <http://data.europa.eu/m8g/> .
@prefix epd: <http://data.europa.eu/a4g/resource/> .
@prefix epo: <http://data.europa.eu/a4g/ontology#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix at-voc-country: <http://publications.europa.eu/resource/authority/country/> .

epd:buyer01 a epo:Buyer;
  epo:playedBy epd:org01 .

epd:org01 a org:Organization;
  epo:hasLegalName "Buyer Org"@en;
  cccev:registeredAddress epd:address01;
  adms:identifier epd:identifier01 .

epd:address01 a locn:Address;
  epo:hasCountryCode at-voc-country:LUX .

epd:identifier01 a adms:Identifier;
  epo:hasScheme "Organization";
  skos:notation "ORG-0001" .

at-voc-country:LUX a skos:Concept .